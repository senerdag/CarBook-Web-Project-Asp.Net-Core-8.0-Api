@model CreateFooterAddressDto

@{
    ViewData["Title"] = "CreateFooterAddress";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


    <div class="page-content">
        <div class="container-fluid">

            <div class="row">
                <div class="col-xl-12">

                    <div class="card">
                        <div class="card-body">

                            <h4 class="card-title">Create New FooterAddress</h4>
                            <p class="card-subtitle mb-4">**Fill the all blanks**</p>

                            <form method="post">
                                <div class="form-group">
                                    <label for="exampleInputAddress">Address</label>
                                    <input type="text" class="form-control" id="exampleInputAddress" asp-for="Adress" placeholder="Address">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPhone">Phone</label>
                                <input type="text" class="form-control" id="exampleInputPhone" asp-for="Phone" placeholder="Phone">
                            </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail">Email</label>
                                <input type="text" class="form-control" id="exampleInputEmail" asp-for="Email" placeholder="Email">
                            </div>
                                <div class="form-group">
                                    <label for="exampleInputDescription">FooterAddress Description</label>
                                    <textarea rows="5" class="form-control" asp-for="Description" placeholder="Description"> </textarea>
                                </div>
                                
                                <button type="submit" class="btn btn-primary waves-effect waves-light">Save FooterAddress</button>
                            </form>

                        </div>
                    </div>

                </div>
            </div>

        </div>

    </div>


